    something starting with __ and the normal name is an alias.  */
#if defined (__stub_getopt_long) || defined (__stub___getopt_long)
choke me
#else
char (*f) () = getopt_long;
#endif
#ifdef __cplusplus
}
#endif

int
main ()
{
return f != getopt_long;
  ;
  return 0;
